<section>
  <div class="container violation-container" role="region" aria-label="{{labeling.dialogMetadata}}">
    <div class="row">
      <div class="col-md-6">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h4>{{labeling.metadataDublinCore}}</h4>
          </div>
          <div class="panel-body">
            <ul class="list-group">
              <li class="list-group-item">
                <strong class="space-right">Identifier: </strong>
                {{#metaData.dc:identifier}}
                  <span class="metadata-success">{{.}}  </span>
                {{/metaData.dc:identifier}}
                {{^metaData.dc:identifier}}
                  <span class="metadata-failed">Missing</span>
                {{/metaData.dc:identifier}}
              </li>
              <li class="list-group-item">
                <strong class="space-right">Language: </strong>
                {{#metaData.dc:language}}
                  <span class="metadata-success">{{.}}  </span>
                {{/metaData.dc:language}}
                {{^metaData.dc:language}}
                  <span class="metadata-failed">Missing</span>
                {{/metaData.dc:language}}
              </li>
              </li>
              <li class="list-group-item">
                <strong class="space-right">Title: </strong>
                {{#metaData.dc:title}}
                  <span class="metadata-success">{{.}}  </span>
                {{/metaData.dc:title}}
                {{^metaData.dc:title}}
                  <span class="metadata-failed">Missing</span>
                {{/metaData.dc:title}}
              </li>
              </li>
              <li class="list-group-item">
                <strong class="space-right">Modified: </strong>
                {{#metaData.dcterms:modified}}
                  <span class="metadata-success">{{.}}  </span>
                {{/metaData.dcterms:modified}}
                {{^metaData.dcterms:modified}}
                  <span class="metadata-failed">Missing</span>
                {{/metaData.dcterms:modified}}
              </li>
              </li>
            </ul>
            <!-- +++++++++++++++ -->
            <!-- metadata accordion  -->
            <!-- +++++++++++++++ -->
            <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
              <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="headingOne">
                  <h4 class="panel-title">
                    <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne"
                       aria-expanded="false" aria-controls="collapseOne">{{labeling.moreMetaData}}</a>
                  </h4>
                </div>
                <div id="collapseOne" class="panel-collapse collapse" role="tabpanel"
                     aria-labelledby="headingOne">
                  <div class="panel-body">
                    {{#metaData.dc:date.length}}
                      <li class="list-group-item">
                        <strong class="space-right">Date: </strong>
                        {{#metaData.dc:date}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:date}}
                      </li>
                    {{/metaData.dc:date.length}}
                    {{#metaData.dc:rights}}
                      <li class="list-group-item"><strong class="space-right">Rights: </strong>
                        <span>{{.}};  </span>
                      </li>
                    {{/metaData.dc:rights}}
                    {{#metaData.dc:creator.length}}
                      <li class="list-group-item"><strong class="space-right">Creator: </strong>
                        {{#metaData.dc:creator}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:creator}}
                      </li>
                    {{/metaData.dc:creator.length}}
                    {{#metaData.dc:contributor.length}}
                      <li class="list-group-item"><strong>Contributors: </strong>
                        {{#metaData.dc:contributor}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:contributor}}
                      </li>
                    {{/metaData.dc:contributor.length}}
                    {{#metaData.dc:source}}
                      <li class="list-group-item"><strong class="space-right">Source: </strong>
                        <span>{{.}};  </span>
                      </li>
                    {{/metaData.dc:source}}
                    {{#metaData.dcterms:conformsTo.length}}
                      <li class="list-group-item"><strong>ConformsTo: </strong>
                        {{#metaData.dcterms:conformsTo}}
                          <span>{{.}};  </span>
                        {{/metaData.dcterms:conformsTo}}
                      </li>
                    {{/metaData.dcterms:conformsTo.length}}
                    {{#metaData.dc:description.length}}
                      <li class="list-group-item"><strong>Description: </strong>
                        {{#metaData.dc:description}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:description}}
                      </li>
                    {{/metaData.dc:description.length}}
                    {{#metaData.dc:format.length}}
                      <li class="list-group-item"><strong>Format: </strong>
                        {{#metaData.dc:format}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:format}}
                      </li>
                    {{/metaData.dc:format.length}}
                    {{#metaData.dc:publisher.length}}
                      <li class="list-group-item"><strong>Publisher: </strong>
                        {{#metaData.dc:publisher}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:publisher}}
                      </li>
                    {{/metaData.dc:publisher.length}}
                    {{#metaData.dc:relation.length}}
                      <li class="list-group-item"><strong>Relation: </strong>
                        {{#metaData.dc:relation}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:relation}}
                      </li>
                    {{/metaData.dc:relation.length}}
                    {{#metaData.dc:subject.length}}
                      <li class="list-group-item"><strong>Subject: </strong>
                        {{#metaData.dc:subject}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:subject}}
                      </li>
                    {{/metaData.dc:subject.length}}
                    {{#metaData.dc:type.length}}
                      <li class="list-group-item"><strong>Type: </strong>
                        {{#metaData.dc:type}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:type}}
                      </li>
                    {{/metaData.dc:type.length}}
                    {{#metaData.dc:coverage.length}}
                      <li class="list-group-item"><strong>Coverage: </strong>
                        {{#metaData.dc:coverage}}
                          <span>{{.}};  </span>
                        {{/metaData.dc:coverage}}
                      </li>
                    {{/metaData.dc:coverage.length}}
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-6">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h4>{{labeling.metadataAccessibility}}</h4>
          </div>
          <div class="panel-body">
            <ul class="list-group">
              <li class="list-group-item">
                <strong class="space-right">AccessMode: </strong>
                {{#metaData.schema:accessMode}}
                  <span class="metadata-success">{{.}}</span>
                {{/metaData.schema:accessMode}}
                {{^metaData.schema:accessMode}}
                  <span class="metadata-failed">Missing</span>
                {{/metaData.schema:accessMode}}
              </li>
              <li class="list-group-item">
                <strong class="space-right">AccessModeSufficient: </strong>
                {{#metaData.schema:accessModeSufficient}}
                  <span class="metadata-success">{{.}}</span>
                {{/metaData.schema:accessModeSufficient}}
                {{^metaData.schema:accessModeSufficient}}
                  <span class="metadata-failed">Missing</span>
                {{/metaData.schema:accessModeSufficient}}
              </li>
              <li class="list-group-item ">
                <strong class="space-right">AccessibilitySummary: <br><br></strong>
                {{#metaData.schema:accessibilitySummary}}
                  <span class="metadata-success">{{metaData.schema:accessibilitySummary}}</span>
                {{/metaData.schema:accessibilitySummary}}
                {{^metaData.schema:accessibilitySummary}}
                  <span class="metadata-failed">Missing</span>
                {{/metaData.schema:accessibilitySummary}}
              </li>
              <li class="list-group-item">
                <strong class="space-right">AccessibilityFeature: </strong>
                {{#metaData.schema:accessibilityFeature}}
                  <span class="metadata-success">{{.}}</span>
                {{/metaData.schema:accessibilityFeature}}
                {{^metaData.schema:accessibilityFeature}}
                  <span class="metadata-failed">Missing</span>
                {{/metaData.schema:accessibilityFeature}}
              </li>
            </ul>
            <!-- +++++++++++++++ -->
            <!-- accessibility metadata accordion  -->
            <!-- +++++++++++++++ -->
            <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
              <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="headingOne">
                  <h4 class="panel-title">
                    <a role="button" data-toggle="collapse" data-parent="#accordion"
                       href="#accessibility-metadata-collapseOne"
                       aria-expanded="false" aria-controls="accessibility-metadata-collapseOne">{{labeling.moreMetaData}}</a>
                  </h4>
                </div>
                <div id="accessibility-metadata-collapseOne" class="panel-collapse collapse" role="tabpanel"
                     aria-labelledby="headingOne">
                  <div class="panel-body">
                    {{#metaData.schema:accessibilityAPI.length}}
                      <li class="list-group-item">
                        <strong class="space-right">AccessibilityAPI: </strong>
                        {{#metaData.schema:accessibilityAPI}}
                          <span>{{.}};  </span>
                        {{/metaData.schema:accessibilityAPI}}
                      </li>
                    {{/metaData.schema:accessibilityAPI.length}}
                    {{#metaData.schema:accessibilityHazard.length}}
                      <li class="list-group-item">
                        <strong class="space-right">AccessibilityHazard: </strong>
                        {{#metaData.schema:accessibilityHazard}}
                          <span>{{.}};  </span>
                        {{/metaData.schema:accessibilityHazard}}
                      </li>
                    {{/metaData.schema:accessibilityHazard.length}}
                    {{#metaData.schema:accessibilityControl.length}}
                      <li class="list-group-item">
                        <strong class="space-right">AccessibilityControl: </strong>
                        {{#metaData.schema:accessibilityControl}}
                          <span>{{.}};  </span>
                        {{/metaData.schema:accessibilityControl}}
                      </li>
                    {{/metaData.schema:accessibilityControl.length}}
                    {{#metaData.a11y:certifierCredential.length}}
                      <li class="list-group-item">
                        <strong class="space-right">CertifierCredential: </strong>
                        {{#metaData.a11y:certifierCredential}}
                          <span>{{.}};  </span>
                        {{/metaData.a11y:certifierCredential}}
                      </li>
                    {{/metaData.a11y:certifierCredential.length}}
                    {{#metaData.a11y:certifiedBy.length}}
                      <li class="list-group-item">
                        <strong class="space-right">CertifiedBy: </strong>
                        {{#metaData.a11y:certifiedBy}}
                          <span>{{.}};  </span>
                        {{/metaData.a11y:certifiedBy}}
                      </li>
                    {{/metaData.a11y:certifiedBy.length}}
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
