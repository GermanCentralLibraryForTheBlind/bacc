<div class="container">
  <div class="row">
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h5 style="display: inline">{{{labeling.tocEPUB}}}</h5>
          <span class="glyphicon glyphicon-question-sign tooltip-active" aria-hidden="true" data-toggle="tooltip"
                data-placement="top"
                title="{{{labeling.tooltipTOC}}}" style="float: right"></span>
          <br>
          {{{labeling.tocEPUBSub}}}
        </div>
        <div class="panel-body">
          {{{outlines.toc}}}
        </div>
      </div>
    </div>
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h5 style="display: inline">{{{labeling.headingsOutline}}}</h5>
          <span class="glyphicon glyphicon-question-sign tooltip-active" aria-hidden="true" data-toggle="tooltip"
                data-placement="top"
                title="{{{labeling.tooltipHeadings}}}" style="float: right"></span>
          <br>
          {{{labeling.headingsOutlineSub}}}
        </div>
        <div class="panel-body">
          {{{outlines.headings}}}
        </div>
      </div>
    </div>
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h5 style="display: inline">{{{labeling.htmlOutline}}}</h5>
          <span class="glyphicon glyphicon-question-sign tooltip-active" aria-hidden="true" data-toggle="tooltip"
                data-placement="top"
                title="{{labeling.tooltipSections}}" style="float: right"></span>
          <br>
          {{{labeling.htmlOutlineSub}}}
        </div>
        <div class="panel-body">
          {{{outlines.html}}}
        </div>
      </div>
    </div>
  </div>
</div>
